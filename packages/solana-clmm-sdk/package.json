{
  "name": "@pancakeswap/solana-clmm-sdk",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.0.0",
  "description": "⚒️ An SDK for building applications on top of Pancakeswap Solana CLMM",
  "files": [
    "dist"
  ],
  "keywords": [
    "pancakeswap",
    "solana",
    "clmm"
  ],
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest --run",
    "update:snapshot": "vitest -u",
    "coverage": "vitest run --coverage",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "dependencies": {
    "@solana/web3.js": "^1.98.2",
    "openapi-fetch": "^0.14.0"
  },
  "devDependencies": {
    "tsup": "^6.7.0",
    "@pancakeswap/utils": "workspace:*",
    "openapi-typescript": "7.8.0",
    "openapi-typescript-helpers": "0.0.15"
  },
  "engines": {
    "node": ">=10"
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  }
}
